# include <iostream>

using namespace std;

int main()
{
    int a[10];
    int n, s = 0;

    cin >> n;

    for (int i = 0; i < n; i++) {
        cin >> a[i];
    }
    for (int i = 0; i < n; i++) {
        if (a[i] % 2 == 1)
            s = a[i];
    }
    for (int i = 0; i < n; i++) {
        if (a[i] % 2 == 1)
            a[i] = s;
    }
    for (int i = 0; i < n; i++) {
        cout << a[i];
    }

    return 0;
}
