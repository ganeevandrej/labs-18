# include <iostream>

using namespace std;

int main()
{
    int a[10];
    float b[10];
    int n;
    float s;

    cin >> n;


    for (int i = 0; i < n; i++) {
        cin >> a[i];
    }
    s = 0;
    for (int i = 1; i < n; i++) {
            s = s + a[i];
            b[i] = s / i;
            cout << b[i] << endl;
    }

    return 0;
}
