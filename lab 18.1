# include <iostream>

using namespace std;

int main()
{
    int a[10], b[10];
    int n;

    cin >> n;


    for (int i = 0; i < n; i++) {
        cin >> a[i];
    }
    for (int i = 0; i < n; i++) {
        cin >> b[i];
    }
    for (int i = 0; i < n; i++) {
        a[i] = a[i] + b[i];
        b[i] = a[i] - b[i];
        a[i] = a[i] - b[i];
    }

    for (int i = 0; i < n; i++)
        cout << a[i];
    cout << endl;
    for (int i = 0; i < n; i++)
        cout << b[i];

    return 0;
}
