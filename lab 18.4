# include <iostream>

using namespace std;

int main()
{
    int a[10];
    int n, min, max;

    cin >> n;

    for (int i = 0; i < n; i++) {
        cin >> a[i];
    }

    max = 1;
    for (int i = 0; i < n; i++) {
        if (a[max] < a[i]) {
            max = i;
        }
    }
    min = max;
    for (int i = 0; i < n; i++) {
        if (a[min] > a[i])
            min = i;
    }
    if (min < max) {
        for (int i = min + 1; i < max; i++) {
            a[i] = 0;
        }
    }
    else
        for (int i = max + 1; i < min; i++) {
            a[i] = 0;
        }
    for (int i = 0; i < n; i++)
        cout << a[i] << endl;
    return 0;
}
